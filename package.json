    "format": "prettier --write . src/lib/db/query/*.sql"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230801.0",
    "@discordjs/collection": "^1.5.2",
    "@miniflare/d1": "^2.14.0",
    "@miniflare/shared": "^2.14.0",
    "@sapphire/snowflake": "^3.5.1",
    "@sveltejs/adapter-cloudflare": "^2.3.2",
    "@sveltejs/kit": "^1.20.4",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "bcryptjs": "^2.4.3",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.30.0",
    "jose": "^4.14.4",
    "meilisearch": "^0.34.0",
    "prettier": "^3.0.1",
    "prettier-plugin-sql": "^0.15.1",
    "prettier-plugin-svelte": "^3.0.3",
    "svelte": "^4.0.5",
    "svelte-check": "^3.4.3",
    "svelte-turnstile": "^0.5.0",
    "tslib": "^2.4.1",
